<h4>All Channels </h4>
<div class="container">
  <table id="all-shows-list" class="table table-striped table-bordered" style="width: 1000px;">
    <thead>
      <th>Channel</th>
      <th>TvShow</th>
      <th>Time</th>
      <th>Favourite</th>
    </thead>
    <tbody>
       <%= render partial: "shared/tvshow", collection: @tv_shows, as: :tvshow %>
    </tbody>
  </table>
</div>
